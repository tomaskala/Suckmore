xwayland disable
exec mako

# Microphone volume
exec wpctl set-volume @DEFAULT_AUDIO_SOURCE@ 100%

# Speaker volume
exec wpctl set-volume @DEFAULT_AUDIO_SINK@ 40%

include ~/.config/sway/config.d/general.conf
include ~/.config/sway/config.d/colorscheme.conf
include ~/.config/sway/config.d/bindings.conf
include ~/.config/sway/config.d/output.conf
include /etc/sway/config.d/*
